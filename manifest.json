{
  "name": "CoinMarketCap Enhanced",
  "version": "1.1.1",
  "manifest_version": 2,
  "description": "Adds a couple enhancements to the cryptocurrency site CoinMarketCap.com",
  "homepage_url": "https://twitter.com/nathanab_",
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://coinmarketcap.com/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://coinmarketcap.com/*"
      ],
      "js": [
        "js/bundle.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "options_page": "options/index.html"
}